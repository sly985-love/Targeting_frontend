.content{
	display: flex;
	flex-direction: column;
	align-items: center;
	height: auto;
}
.top{
	width: 100%;
	height: 360rpx;
	/* background-color: #4CD964; */
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: flex-end;
	font-size: 60rpx;
	padding-bottom: 30rpx;
}
.title{
	margin-top: 20rpx;
	font-size: 34rpx;
	color: #555555;
}
.middle{
	display: flex;
	flex-direction: column;
	height: 600rpx;
}
#card{
	width: 600rpx;
	height: 150rpx;
	margin: 30rpx;
	border-radius: 14rpx;
	box-shadow: #C0C0C0 4rpx 4rpx 5rpx; 
	display: flex;
	align-items: center;
	justify-content: center;
	color: white;
	font-size: 40rpx;
}
#logo_wapper{
	width: 100%;
	height: 340rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}
.logo {
		width: 280rpx;
	    height: 280rpx;
}

