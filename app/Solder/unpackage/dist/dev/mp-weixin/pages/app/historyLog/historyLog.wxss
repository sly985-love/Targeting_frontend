.header{
	width: 100%;
	height: 180rpx;
	/* background-color: #18BC37; */
	font-size: 60rpx;
	display: flex;
	justify-content: center;
	align-items: center;
}
.hb{
	width: 100%;
	height:420rpx;
}
.grade_wapper{
	width: 100%;
	height: auto;
	margin-top: 20rpx;
}
.title_wapper{
	width: 100%;
	height: 100rpx;
	/* background-color: #18BC37; */
	font-size: 38rpx;
	display: flex;
	align-items: center;
	/* margin-left: 30upx; */
	box-shadow: #DCDCDC 1rpx 2rpx 2rpx;
	border-bottom: #DCDCDC 1rpx solid;
}
#smallIcon{
	margin-left: 30rpx;
}
.grade_content{
	width: 100%;
	height: 520rpx;
}
.fenxi{
	width: 100%;
	height: 500rpx;
	margin-top: 20rpx;
	padding-bottom: 200rpx;
}
.fenxi_wapper{
	display: flex;
	justify-content: center;
	flex-wrap: wrap;  /* 换行 */
}
.single_fenxi{
	width: 45%;
	height: 200rpx;
	margin: 10rpx;
	margin-top: 24rpx;
	box-shadow: #C7C7C7 4rpx 4rpx 4rpx;
	border: #C7C7C7 1rpx solid;
}
.fenxi_data{
	width: 100%;
	height: 140rpx;
	font-size: 46rpx;
	font-weight: bolder;
	color:#DD524D;
	display: flex;
	justify-content: center;
	align-items: center;
}
.fenxi_title{
	width: 100%;
	height: 60rpx;
	background-color: #ffa397;
	color: white;
	font-size: 34rpx;
	font-weight: 500;
	display: flex;
	align-items: center;
	justify-content: center;
}

